<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Paquetes</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>me.ujosue.servlet.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/Login.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Registrar</servlet-name>
    <servlet-class>me.ujosue.servlet.Registrar</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Registrar</servlet-name>
    <url-pattern>/Registrar.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>NuevoPaquete</servlet-name>
    <servlet-class>me.ujosue.servlet.NuevoPaquete</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>NuevoPaquete</servlet-name>
    <url-pattern>/NuevoPaquete.do</url-pattern>
  </servlet-mapping>
</web-app>